{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
	
		{
			"path": "pages/index/index",
			"style": {
				"navigationStyle": "custom",
				"enablePullDownRefresh": true
			}
		}
	    ,{
            "path" : "pages/404/404",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "页面未找到",
                "enablePullDownRefresh": false
            }
            
        },{
            "path" : "pages/me/me",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "",
                "enablePullDownRefresh": false
            }
            
        },{
            "path" : "pages/order/order",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "",
                "enablePullDownRefresh": false
            }
            
        }
    ],
	"subPackages": [{
			"root": "subpages",
			"pages": [{
					"path": "ad/ad",
					"style": {
						"navigationBarTitleText": "广告",
						"enablePullDownRefresh": false
					}
				}]
		}],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar":{
		"color":"#7a7f82",
		"selectedColor": "#3cc51f",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list":[{
			"pagePath":"pages/index/index",
			"iconPath":"static/tabbar-icons/首页.png",
			"selectedIconPath": "static/tabbar-icons/首页选中.png",
			"text":"首页"
		},{
			"pagePath":"pages/order/order",
			"iconPath":"static/tabbar-icons/订单.png",
			"selectedIconPath": "static/tabbar-icons/订单选中.png",
			"text":"订单"
		},{
			"pagePath":"pages/me/me",
			"iconPath":"static/tabbar-icons/我的.png",
			"selectedIconPath": "static/tabbar-icons/我的_选中.png",
			"text":"我的"
		}]
	},
	"uniIdRouter": {},
	"usingComponents":{
		"van-button":"@vant/weapp/button/index"
	}
}
